<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.common.dao.RiskCommonDao">
	<resultMap id="utiFactorMap" type="com.vo.UtiFactor" >
	     <!-- 设置联合主键 -->
		<id column="riskmodel" property="riskModel" jdbcType="VARCHAR"/>
		<id column="factorno" property="factorNo" jdbcType="VARCHAR"/>
		<id column="dangertype" property="dangerType" jdbcType="VARCHAR"/>
		
		<result column="factortype" property="factorType" jdbcType="VARCHAR"/>
		
	</resultMap>
	
	<select id="getUtiFactorList" resultMap="utiFactorMap"  parameterType="java.util.HashMap">
		select * from  utifactor
		<where>
			<include refid="queryUtiFactor"></include>
		</where>
	</select>
	
	<sql id="queryUtiFactor">
		<if test="riskModel !=null and riskModel!=''">
			and riskModel= #{riskModel}
		</if> 
		<if test="dangerType !=null and dangerType !=''">
			and dangerType in (#{dangerType})
		</if>
	</sql>
	
	
	
</mapper>